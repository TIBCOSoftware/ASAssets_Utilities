  <attribute name="Script" type="STRING">PROCEDURE Test_ALL(
    OUT totalNumTests INTEGER,
    OUT totalNumErrors INTEGER,
    OUT totalNumSkipped INTEGER
)
BEGIN
    DECLARE test_name VARCHAR DEFAULT &apos;string&apos;;
    DECLARE numTests INTEGER;
    DECLARE numErrors INTEGER;
    DECLARE numSkipped INTEGER;

    SET totalNumTests = 0;
    SET totalNumErrors = 0;
    SET totalNumSkipped = 0;

    CALL PRINT (test_name || &apos;: Beginning tests.&apos;);

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_addQuotesInList (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_basename (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_dirname (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_emptyStr (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_escapeCSV (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_findString (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_fixQuotes (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_isEmpty (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_last4ofSSN (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_p_DelimitedStringToCursor (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_p_FixedStringToCursor (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_ParseCSVLine (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/Test_removeSingleQuotes (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_ALL (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL PRINT (test_name || &apos;: Tests complete: &apos; || totalNumTests || &apos; total tests, &apos; || totalNumErrors || &apos; total errors, &apos; || totalNumSkipped || &apos; skipped tests.&apos;);

    IF (totalNumErrors &gt; 0) THEN
        CALL PRINT (&apos;***** Total Errors = &apos; || totalNumErrors);
    END IF;

    IF (totalNumSkipped &gt; 0) THEN
        CALL PRINT (&apos;***** Total Skipped = &apos; || totalNumSkipped);
    END IF;
END</attribute>
