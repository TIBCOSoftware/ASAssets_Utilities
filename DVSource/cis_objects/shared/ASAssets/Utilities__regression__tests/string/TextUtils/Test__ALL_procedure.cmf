  <attribute name="Script" type="STRING">PROCEDURE Test_ALL(
    OUT totalNumTests INTEGER,
    OUT totalNumErrors INTEGER,
    OUT totalNumSkipped INTEGER
)
BEGIN
    DECLARE test_name VARCHAR DEFAULT &apos;string/TextUtils&apos;;
    DECLARE numTests INTEGER;
    DECLARE numErrors INTEGER;
    DECLARE numSkipped INTEGER;

    SET totalNumTests = 0;
    SET totalNumErrors = 0;
    SET totalNumSkipped = 0;

    CALL PRINT (test_name || &apos;: Beginning tests.&apos;);

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_CCNumberFormatter (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    -- something about these tests causes CIS to spin the CPU and requires a CIS restart to recover. Weird.
    --
/*
    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_CSVFromCISQuery (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_CSVFromCISQueryToFile (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_FixedFromCISQuery (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_FixedFromCISQueryToFile (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;
*/
    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalCurrencyFormatter (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalCurrencyParser (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalDateFormatter (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalDateParser (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalNumberFormatter (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalNumberParser (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalTimeFormatter (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalTimeParser (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalTimestampFormatter (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_LocalTimestampParser (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/string/TextUtils/Test_PhoneNumberFormatter (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
