    BEGIN
        SET numTests = numTests + 1;
        CALL /shared/ASAssets/Utilities_regression_tests/TestUtils/QueryTest(
            &apos;SELECT convertBit(&apos; || args || &apos;) result FROM /services/databases/system/ALL_DOMAINS where DOMAIN_NAME = &apos;&apos;composite&apos;&apos;&apos;,
