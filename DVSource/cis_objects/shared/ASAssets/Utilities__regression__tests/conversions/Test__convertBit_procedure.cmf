  <attribute name="Script" type="STRING">PROCEDURE Test_convertBit(
    OUT numTests INTEGER,
    OUT numErrors INTEGER,
    OUT numSkipped INTEGER
)
BEGIN
    DECLARE test_name VARCHAR DEFAULT &apos;conversions/Test_convertBit&apos;;
    DECLARE args System.Text;
    DECLARE expectedResultCSV System.Text;
    DECLARE result System.Text;

    SET numTests = 0;
    SET numErrors = 0;
    SET numSkipped = 0;

    -- start test
    --
    SET args = &apos;&apos;&apos;true&apos;&apos;&apos;;
    SET expectedResultCSV = &apos;1&apos;;

    BEGIN
        SET numTests = numTests + 1;
        CALL /shared/ASAssets/Utilities_regression_tests/TestUtils/QueryTest(
            &apos;SELECT convertBit(&apos; || args || &apos;) result FROM /services/databases/system/ALL_DOMAINS where DOMAIN_NAME = &apos;&apos;composite&apos;&apos;&apos;,
            expectedResultCSV,
            result
        );
    EXCEPTION
        ELSE
            SET result = &apos;EXCEPTION: &apos; || CURRENT_EXCEPTION.MESSAGE;
    END;

    CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: &apos; || result);

    IF (result &lt;&gt; &apos;OK&apos;) THEN
        SET numErrors = numErrors + 1;
    END IF;
    --
    -- end test

    -- start test
