        ELSE
            SET result = &apos;EXCEPTION: &apos; || CURRENT_EXCEPTION.MESSAGE;
            SET numErrors = numErrors + 1;
    END;

    CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: &apos; || result);

