  <attribute name="Script" type="STRING">PROCEDURE Test_ALL(
    OUT totalNumTests INTEGER,
    OUT totalNumErrors INTEGER,
    OUT totalNumSkipped INTEGER
)
BEGIN
    DECLARE test_name VARCHAR DEFAULT &apos;file/FileProcessingCJP&apos;;
    DECLARE numTests INTEGER;
    DECLARE numErrors INTEGER;
    DECLARE numSkipped INTEGER;

    SET totalNumTests = 0;
    SET totalNumErrors = 0;
    SET totalNumSkipped = 0;

    CALL PRINT (test_name || &apos;: Beginning tests.&apos;);

    -- instead of testing in alphabetical order like the other utilities,
    -- the file processing CJP&apos;s need to be tested in a particular order.
    -- testing also needs to stop if certain tests fail.
    --
    test_block:
    BEGIN

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_existsDir (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;
        IF (totalNumErrors &gt; 0) THEN LEAVE test_block; END IF; -- tests can&apos;t progress if this fails

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_existsFile (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;
        IF (totalNumErrors &gt; 0) THEN LEAVE test_block; END IF; -- tests can&apos;t progress if this fails

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_getFileInfo (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_getFileContentsAscii (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_makeDirs (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;
        IF (totalNumErrors &gt; 0) THEN LEAVE test_block; END IF; -- tests can&apos;t progress if this fails

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_createFileAscii (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;
        IF (totalNumErrors &gt; 0) THEN LEAVE test_block; END IF; -- tests can&apos;t progress if this fails

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_copyFile (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_archiveFile (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_remove (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;

        CALL /shared/ASAssets/Utilities_regression_tests/file/FileProcessingCJP/Test_removeAll (numTests, numErrors, numSkipped);
        SET totalNumTests = totalNumTests + numTests;
        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;

    END; -- test_block

    CALL PRINT (test_name || &apos;: Tests complete: &apos; || totalNumTests || &apos; total tests, &apos; || totalNumErrors || &apos; total errors, &apos; || totalNumSkipped || &apos; skipped tests.&apos;);

    IF (totalNumErrors &gt; 0) THEN
        CALL PRINT (&apos;***** Total Errors = &apos; || totalNumErrors);
    END IF;

    IF (totalNumSkipped &gt; 0) THEN
        CALL PRINT (&apos;***** Total Skipped = &apos; || totalNumSkipped);
    END IF;
END</attribute>
