        SET totalNumErrors = totalNumErrors + numErrors;
        SET totalNumSkipped = totalNumSkipped + numSkipped;

        CALL /shared/ASAssets/Utilities_regression_tests/repository/Test_updateResourcesSqlTable (numTests, numErrors, numSkipped);
