  <attribute name="Script" type="STRING">-- resources created:
--
-- DATA_SOURCE        Constants.TMP_FOLDER/test_ds_mysql
--
PROCEDURE Test_createDataSource(
    OUT numTests INTEGER,
    OUT numErrors INTEGER,
    OUT numSkipped INTEGER
)
BEGIN
    PATH /shared/ASAssets/Utilities_regression_tests;
    PATH /lib/resource;

    DECLARE test_name VARCHAR DEFAULT &apos;repository/Test_createDatasource&apos;;
    DECLARE parentFolder ResourceDefs.ResourcePath;
    DECLARE dataSourceName ResourceDefs.ResourceName;
    DECLARE dataSourceType ResourceDefs.ResourceType;
    DECLARE args LONGVARCHAR;
    DECLARE success BIT;
    DECLARE responseXML XML;
    DECLARE faultXML XML;
    DECLARE resourceExists BOOLEAN;

    SET numTests = 0;
    SET numErrors = 0;
    SET numSkipped = 0;

    -- start test
    --
    test_block_1: 
    BEGIN INDEPENDENT TRANSACTION
        SET parentFolder = /shared/ASAssets/Utilities_regression_tests/Constants.TMP_FOLDER;

        CASE (SUBSTRING(getCisVersion(), 1, 3))
            WHEN &apos;6.2&apos; THEN
                SET dataSourceName = &apos;test_ds_mysql&apos;;
                SET dataSourceType = &apos;MySQL 5.0&apos;;
            ELSE
                SET dataSourceName = &apos;test_ds_postgresql&apos;;
                SET dataSourceType = &apos;PostgreSQL 9.1&apos;;
        END CASE;

        SET args = &apos;&apos;&apos;&apos; || parentFolder || &apos;&apos;&apos;, &apos; ||
                   &apos;&apos;&apos;&apos; || dataSourceName || &apos;&apos;&apos;, &apos; ||
                   &apos;&apos;&apos;&apos; || dataSourceType || &apos;&apos;&apos;&apos;;

        SET numTests = numTests + 1;

        CALL /lib/resource/ResourceExists(
             parentFolder || &apos;/&apos; || dataSourceName,
             ResourceDefs.RESOURCE_TYPE_DATA_SOURCE,
             NULL,
             resourceExists
        );

        IF (resourceExists) THEN
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: SKIPPING, resource exists&apos;);
            SET numSkipped = numSkipped + 1;
            LEAVE test_block_1;
        END IF;

        CALL /shared/ASAssets/Utilities/repository/createDataSource(
            parentFolder,
            dataSourceName,
            dataSourceType,
            success,
            responseXML,
            faultXML
        );

        IF (success &lt;&gt; 1) THEN
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: FAILED&apos;);
            SET numErrors = numErrors + 1;
            LEAVE test_block_1;
        END IF;

        CALL /lib/resource/ResourceExists(
             parentFolder || &apos;/&apos; || dataSourceName,
             ResourceDefs.RESOURCE_TYPE_DATA_SOURCE,
             NULL,
             resourceExists
        );

        IF (resourceExists) THEN
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: OK&apos;);
        ELSE
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: FAILED&apos;);
            SET numErrors = numErrors + 1;
        END IF;

    EXCEPTION
        ELSE
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: EXCEPTION: &apos; || CURRENT_EXCEPTION.MESSAGE);
            SET numErrors = numErrors + 1;
    END; -- test_block_1
    --
    -- end test

END</attribute>
