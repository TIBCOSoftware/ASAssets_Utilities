  <attribute name="Script" type="STRING">PROCEDURE Test_rebindResource(
    OUT numTests INTEGER,
    OUT numErrors INTEGER,
    OUT numSkipped INTEGER
)
proc:
BEGIN
    PATH /shared/ASAssets/Utilities_regression_tests;
    PATH /lib/resource;

    DECLARE test_name VARCHAR DEFAULT &apos;repository/Test_rebindResource&apos;;
    DECLARE resourcePath ResourceDefs.ResourcePath;
    DECLARE resourceType ResourceDefs.ResourceType;
    DECLARE oldResourcePath ResourceDefs.ResourcePath;
    DECLARE oldResourceType ResourceDefs.ResourceType;
    DECLARE newResourcePath ResourceDefs.ResourcePath;
    DECLARE newResourceType ResourceDefs.ResourceType;
    DECLARE args LONGVARCHAR;
    DECLARE success BIT;
    DECLARE responseXML XML;
    DECLARE faultXML XML;
    DECLARE resourceExists BOOLEAN;
    DECLARE testSqlText LONGVARCHAR;

    SET numTests = 0;
    SET numErrors = 0;
    SET numSkipped = 0;

    -- start test
    --
    test_block_1: 
    BEGIN INDEPENDENT TRANSACTION
        SET resourcePath = /shared/ASAssets/Utilities_regression_tests/Constants.TMP_FOLDER || &apos;/test_folder/test_view&apos;; -- created in Test_createResource
        SET resourceType = ResourceDefs.RESOURCE_TYPE_TABLE;
        SET oldResourceType = ResourceDefs.RESOURCE_TYPE_TABLE;
        SET newResourceType = ResourceDefs.RESOURCE_TYPE_TABLE;

	CASE (SUBSTRING(getCisVersion(), 1, 3))
	    WHEN &apos;6.2&apos; THEN
            SET oldResourcePath = &apos;/shared/examples/ds_orders/orders&apos;;
            SET newResourcePath = &apos;/shared/examples/ds_orders/orders_cache&apos;;
        ELSE
            SET oldResourcePath = &apos;/shared/examples/ds_orders/tutorial/orders&apos;;
            SET newResourcePath = &apos;/shared/examples/ds_orders/tutorial/orders_cache&apos;;
    END CASE;

        SET args = &apos;&apos;&apos;&apos; || resourcePath || &apos;&apos;&apos;, &apos; ||
                   &apos;&apos;&apos;&apos; || resourceType || &apos;&apos;&apos;, &apos; ||
                   &apos;VECTOR [(&apos; ||
                   &apos;&apos;&apos;&apos; || oldResourcePath || &apos;&apos;&apos;, &apos; ||
                   &apos;&apos;&apos;&apos; || oldResourceType || &apos;&apos;&apos;, &apos; ||
                   &apos;&apos;&apos;&apos; || newResourcePath || &apos;&apos;&apos;, &apos; ||
                   &apos;&apos;&apos;&apos; || newResourceType || &apos;&apos;&apos;&apos; ||
                   &apos;)]&apos;;

        SET numTests = numTests + 1;

        CALL /lib/resource/ResourceExists(
             resourcePath,
             ResourceDefs.RESOURCE_TYPE_TABLE,
             NULL,
             resourceExists
        );

        IF (NOT resourceExists) THEN
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: SKIPPING, resource does not exist&apos;);
            SET numSkipped = numSkipped + 1;
            LEAVE test_block_1;
        END IF;

        CALL /shared/ASAssets/Utilities/repository/rebindResource(
            resourcePath,
            resourceType,
            VECTOR [(
                oldResourcePath,
                oldResourceType,
                newResourcePath,
                newResourceType
            )],
            success,
            responseXML,
            faultXML
        );

        IF (success &lt;&gt; 1) THEN
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: FAILED&apos;);
            SET numErrors = numErrors + 1;
            LEAVE test_block_1;
        END IF;

        SELECT
            sqlText
        INTO
            testSqlText
        FROM 
            /shared/ASAssets/Utilities/repository/getBasicResourceCursor_SQL_TABLE(
                resourcePath,
                resourceType
            )
        WHERE
            columnName IS NULL;

        IF (POSITION (newResourcePath IN testSqlText) &gt; 0) THEN
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: OK&apos;);
        ELSE
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: FAILED&apos;);
            SET numErrors = numErrors + 1;
        END IF;

    EXCEPTION
        ELSE
            CALL PRINT (test_name || &apos;: args = &apos; || args || &apos;: EXCEPTION: &apos; || CURRENT_EXCEPTION.MESSAGE);
            SET numErrors = numErrors + 1;
    END; -- test_block_1
    --
    -- end test

END</attribute>
