  <attribute name="Script" type="STRING">PROCEDURE Test_ALL(
    OUT totalNumTests INTEGER,
    OUT totalNumErrors INTEGER,
    OUT totalNumSkipped INTEGER
)
BEGIN
    DECLARE test_name VARCHAR DEFAULT &apos;encoding/EncodingCJP&apos;;
    DECLARE numTests INTEGER;
    DECLARE numErrors INTEGER;
    DECLARE numSkipped INTEGER;

    SET totalNumTests = 0;
    SET totalNumErrors = 0;
    SET totalNumSkipped = 0;

    CALL PRINT (test_name || &apos;: Beginning tests.&apos;);

    CALL /shared/ASAssets/Utilities_regression_tests/encoding/EncodingCJP/Test_Base64Decode (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/encoding/EncodingCJP/Test_Base64Encode (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/encoding/EncodingCJP/Test_DecryptFrom3DES (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/encoding/EncodingCJP/Test_DecryptWithCISPrivKey (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/encoding/EncodingCJP/Test_EncryptWith3DES (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

/* this doesn&apos;t work. even using the same arguments, the result of EncryptWithCISPubKey changes every time it&apos;s run.
    CALL /shared/ASAssets/Utilities_regression_tests/encoding/EncodingCJP/Test_EncryptWithCISPubKey (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;
*/

    CALL /shared/ASAssets/Utilities_regression_tests/encoding/EncodingCJP/Test_MD5Hash (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL /shared/ASAssets/Utilities_regression_tests/encoding/EncodingCJP/Test_SHA1Hash (numTests, numErrors, numSkipped);
    SET totalNumTests = totalNumTests + numTests;
    SET totalNumErrors = totalNumErrors + numErrors;
    SET totalNumSkipped = totalNumSkipped + numSkipped;

    CALL PRINT (test_name || &apos;: Tests complete: &apos; || totalNumTests || &apos; total tests, &apos; || totalNumErrors || &apos; total errors, &apos; || totalNumSkipped || &apos; skipped tests.&apos;);

    IF (totalNumErrors &gt; 0) THEN
        CALL PRINT (&apos;***** Total Errors = &apos; || totalNumErrors);
    END IF;

    IF (totalNumSkipped &gt; 0) THEN
        CALL PRINT (&apos;***** Total Skipped = &apos; || totalNumSkipped);
    END IF;
END</attribute>
