PROCEDURE deleteConnector(
	IN debug 				CHAR(1),-- e.g. Y or N
	IN CONN_NAME 			VARCHAR(100)
)
BEGIN
	DECLARE moduleName 		VARCHAR DEFAULT &apos;deleteConnector&apos;;
	DECLARE request 		VARCHAR(4096);
	DECLARE response 		XML;
	DECLARE fault 			XML;
    DECLARE ex				EXCEPTION;

	set request =
		&apos;&lt;ns1:destroyConnector xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/resource&quot; 
			xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot; 
			xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt; 
			   &lt;ns1:name&gt;&apos; || CAST(XMLTEXT(CONN_NAME) AS LONGVARCHAR) || &apos;&lt;/ns1:name&gt; 
		&lt;/ns1:destroyConnector&gt;&apos;;
	CALL /shared/ASAssets/Utilities/log/logDebugMessage(moduleName, debug, request);
	CALL /services/webservices/system/admin/resource/operations/destroyConnector(request, response, fault);
	IF (fault IS NOT NULL) THEN
		raise ex value CAST(fault as longvarchar);
	END IF;

	EXCEPTION
	  ELSE
		call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, emptyStr(CURRENT_EXCEPTION.TRACE));
		raise ex value CURRENT_EXCEPTION.TRACE;
END
