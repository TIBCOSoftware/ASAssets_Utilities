PROCEDURE updateTableColumnStatisticsConfiguration(
	IN resourcePath /lib/resource/ResourceDefs.ResourcePath,
	IN cardinalityMin INTEGER,
	IN cardinalityMax INTEGER,
	IN cardinalityExpected INTEGER,
	IN gatherEnabled VARCHAR(20),
	IN maxTime INTEGER,
	IN columnSettings VECTOR (ColumnSettingsRow),
	OUT responseXML XML,
	OUT faultXML XML
)
BEGIN
    DECLARE PUBLIC TYPE ColumnSettingsRow ROW(
		name VARCHAR(1000),
		flags VARCHAR(1000),
		columnMin DOUBLE,
		columnMax DOUBLE,
		columnDistinct INTEGER
    );

	DECLARE requestXML LONGVARCHAR;
	DECLARE i INTEGER DEFAULT 1;

	IF resourcePath IS NULL THEN
		RAISE System.IllegalArgumentException VALUE &apos;Resource path may not be null!&apos;;
	END IF;

	SET requestXML = 
&apos;&lt;resource:updateResourceStatisticsConfig xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot; xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;
  &lt;resource:path&gt;&apos; || resourcePath || &apos;&lt;/resource:path&gt;
  &lt;resource:type&gt;TABLE&lt;/resource:type&gt;
  &lt;resource:detail&gt;NONE&lt;/resource:detail&gt;
  &lt;resource:statisticsConfig&gt;&apos;;

	IF cardinalityMin IS NOT NULL THEN
		SET requestXML = requestXML || &apos;
    &lt;resource:cardinalityMin&gt;&apos; || cardinalityMin || &apos;&lt;/resource:cardinalityMin&gt;&apos;;
	END IF;

	IF cardinalityMax IS NOT NULL THEN
		SET requestXML = requestXML || &apos;
    &lt;resource:cardinalityMax&gt;&apos; || cardinalityMax || &apos;&lt;/resource:cardinalityMax&gt;&apos;;
	END IF;

	IF cardinalityExpected IS NOT NULL THEN
		SET requestXML = requestXML || &apos;
    &lt;resource:cardinalityExpected&gt;&apos; || cardinalityExpected || &apos;&lt;/resource:cardinalityExpected&gt;&apos;;
	END IF;

	IF gatherEnabled IS NOT NULL THEN
		SET requestXML = requestXML || &apos;
    &lt;resource:gatherEnabled&gt;&apos; || gatherEnabled || &apos;&lt;/resource:gatherEnabled&gt;&apos;;
	END IF;

	IF maxTime IS NOT NULL THEN
		SET requestXML = requestXML || &apos;
    &lt;resource:maxTime&gt;&apos; || maxTime || &apos;&lt;/resource:maxTime&gt;&apos;;
	END IF;

	IF columnSettings IS NOT NULL THEN
		SET requestXML = requestXML || &apos;
    &lt;resource:columns&gt;&apos;;

		WHILE i &lt;= CARDINALITY(columnSettings) DO
			IF columnSettings[i].name IS NULL OR columnSettings[i].flags IS NULL THEN
				RAISE System.IllegalArgumentException VALUE &apos;Column name and flags values may not be null!&apos;;
			END IF;

			SET requestXML = requestXML || &apos;
      &lt;resource:column&gt;
        &lt;resource:name&gt;&apos; || columnSettings[i].name || &apos;&lt;/resource:name&gt;
        &lt;resource:flags&gt;&apos; || columnSettings[i].flags || &apos;&lt;/resource:flags&gt;&apos;;

			IF columnSettings[i].columnMin IS NOT NULL THEN
				SET requestXML = requestXML || &apos;
        &lt;resource:columnMin&gt;&apos; || columnSettings[i].columnMin || &apos;&lt;/resource:columnMin&gt;&apos;;
			END IF;

			IF columnSettings[i].columnMax IS NOT NULL THEN
				SET requestXML = requestXML || &apos;
        &lt;resource:columnMax&gt;&apos; || columnSettings[i].columnMax || &apos;&lt;/resource:columnMax&gt;&apos;;
			END IF;

			IF columnSettings[i].columnDistinct IS NOT NULL THEN
				SET requestXML = requestXML || &apos;
        &lt;resource:columnDistinct&gt;&apos; || columnSettings[i].columnDistinct || &apos;&lt;/resource:columnDistinct&gt;&apos;;
			END IF;

			SET requestXML = requestXML || &apos;
      &lt;/resource:column&gt;&apos;;

			SET i = i + 1;
		END WHILE;

		SET requestXML = requestXML || &apos;
    &lt;/resource:columns&gt;&apos;;
	END IF;

	SET requestXML = requestXML || &apos;
  &lt;/resource:statisticsConfig&gt;
&lt;/resource:updateResourceStatisticsConfig&gt;&apos;;

	--CALL print(requestXML);

	CALL /services/webservices/system/admin/resource/operations/updateResourceStatisticsConfig(requestXML, responseXML, faultXML);
END</attribute>
