PROCEDURE importResourcePrvileges(
	IN updateRecursively BIT,
	IN updateDependenciesRecursively BIT,
	IN updateDependentsRecursively BIT,
	IN fileName LONGVARCHAR,
	IN mode VARCHAR,
    OUT updateResourcePrivilegesResponse XML, 
    OUT fault XML
