    csserverutil-XXXX.jar, csserver-XXXX.jar, cswebapi-XXXX.jar, and customproc-XXXX.jar (where -XXXX is an 
    optional version number) are provided as a convenience, but are covered under the licensing for the 
    Composite Information Server/Cisco Data Virtualization Server. They cannot be used in any way except 
    through a valid license for that product.

    This software is released AS-IS!. Support for this software is not covered by standard maintenance agreements with Cisco. 
    Any support for this software by Cisco would be covered by paid consulting agreements, and would be billable work.

*/
PROCEDURE createResourcePrivilege(
	IN debug 				CHAR(1),-- e.g. Y or N
	IN owner				VARCHAR(255),-- Owner should be set to the name of the user which should own all the objects
	IN ownerDomain			VARCHAR(255),-- Domain for owner - default is &apos;composite&apos;
	IN resourcePath			/lib/resource/ResourceDefs.ResourcePath,-- full resource path to the composite resource
	IN resourceType 		/lib/resource/ResourceDefs.ResourceType,-- resource type e.g. CONTAINER, DATA_SOURCE, PROCEDURE, VIEW
	IN recurse 				CHAR(1),-- 1=recurse through folders if the resource is a folder, 0=no recurse
	IN name					VARCHAR(255),-- name of the user or group to assign the privilege
	IN domainName			VARCHAR(255),-- composite domain
	IN nameType				VARCHAR(255),-- USER or GROUP
	IN &quot;privileges&quot;			VARCHAR(255)-- space separated list of privileges: READ SELECT EXECUTE INSERT UPDATE DELETE
)
BEGIN
	DECLARE moduleName 		VARCHAR DEFAULT &apos;createResourcePrivilege&apos;;
	DECLARE request 		VARCHAR(4096);
	DECLARE response 		XML;
	DECLARE fault 			XML;
    DECLARE user_exists 	BOOLEAN;
    DECLARE groups 			VECTOR(VARCHAR) DEFAULT VECTOR[];
	DECLARE message 		LONGVARCHAR;
	DECLARE padChar			CHAR(1) DEFAULT &apos; &apos;;
    DECLARE i,j				INTEGER DEFAULT 1;
    DECLARE ex 				EXCEPTION;
   /*
    * The resourcelist is an array of resources - usually folders (CONTAINER) and virtual
    * databases (DATA_SOURCE).  For each resource, an array is provided with the users or
    * groups who can access that resource, and a set of resource privileges for each.
    * If the recurse flag is &apos;1&apos; the same permissions will be applied to sub-resources.
    */
    DECLARE resourcelist CONSTANT VECTOR(
        ROW(resource VARCHAR, resourceType VARCHAR, recurse CHAR(1), resourceprivs VECTOR(
            ROW(name VARCHAR, domainname VARCHAR, nametype VARCHAR, privs VARCHAR))
        ))
        DEFAULT VECTOR[(resourcePath, resourceType, recurse, VECTOR[(name, domainName, nameType, REPLACE(&quot;privileges&quot;,&apos;|&apos;,&apos; &apos;))])];
/*
(&apos;/shared&apos;, &apos;CONTAINER&apos;, &apos;1&apos;, VECTOR[ (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE&apos;) ]),
(&apos;/shared/BestPractices&apos;, &apos;CONTAINER&apos;, &apos;1&apos;, VECTOR[  (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)    ]),
(&apos;/shared/Utilities&apos;, &apos;CONTAINER&apos;, &apos;1&apos;, VECTOR[      (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)    ]),

(&apos;/services/databases/VIPSAPP&apos;, &apos;DATA_SOURCE&apos;, &apos;1&apos;, VECTOR[(&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;) ]),
(&apos;/shared/VIPS_02&apos;, &apos;CONTAINER&apos;, &apos;1&apos;, VECTOR[ (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)  ]),
(&apos;/shared/VIPS_03&apos;, &apos;CONTAINER&apos;, &apos;1&apos;, VECTOR[ (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)  ]),
(&apos;/shared/VIPS_DataSources&apos;, &apos;CONTAINER&apos;, &apos;1&apos;, VECTOR[ (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)  ]),


(&apos;/services/webservices/spiral1&apos;, &apos;CONTAINER&apos;, &apos;1&apos;, VECTOR[  (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)   ]),
(&apos;/services/webservices/vipsApplicationWS_v1_1&apos;, &apos;DATA_SOURCE&apos;, &apos;1&apos;, VECTOR[  (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)   ]),
(&apos;/services/webservices/vipsMedicalPrescreenFormWS_v1_1&apos;, &apos;DATA_SOURCE&apos;, &apos;1&apos;, VECTOR[ (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)    ]),
(&apos;/services/webservices/vipsMedicalPrescreenWS_v1_1&apos;, &apos;DATA_SOURCE&apos;, &apos;1&apos;, VECTOR[  (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)     ]),
(&apos;/services/webservices/vipsStatusWS_v1_1&apos;, &apos;DATA_SOURCE&apos;, &apos;1&apos;, VECTOR[  (&apos;vipsuser&apos;, &apos;composite&apos;, &apos;GROUP&apos;, &apos;READ SELECT EXECUTE INSERT UPDATE DELETE&apos;)    ])    ];
*/

/*
    * Step three: Display the existing privileges
 		   OUT result CURSOR (
				&quot;domain&quot; VARCHAR(32768), 
				name VARCHAR(32768), 
				nameType VARCHAR(32768), 
				privs VARCHAR(32768), 
				combinedPrivs VARCHAR(32768), 
				inheritedPrivs VARCHAR(32768)
   		     )
*/
    SET i = 1;
    WHILE i &lt;= CARDINALITY(resourcelist)
    DO
        call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, &apos;EXISTING PRIVILEGES FOR &apos; || resourcelist[i].resource);
		set message = 	RPAD(&apos;domain&apos;,12,padChar) ||&apos;  &apos;||
						RPAD(&apos;name&apos;,12,padChar) || &apos;  &apos;||
						RPAD(&apos;nameType&apos;,10,padChar) || &apos;  &apos;||
						RPAD(&apos;privs&apos;,60,padChar) || &apos;  &apos;||
						RPAD(&apos;combinedPrivs&apos;,60,padChar) || &apos;  &apos;||
						RPAD(&apos;inheritedPrivs&apos;,60,padChar);							
		call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, &apos;EXISTING PRIVILEGES:&apos; || message);
		set message = 	RPAD(&apos;------------&apos;,12,padChar) ||&apos;  &apos;||
						RPAD(&apos;------------&apos;,12,padChar) || &apos;  &apos;||
						RPAD(&apos;----------&apos;,10,padChar) || &apos;  &apos;||
						RPAD(&apos;------------------------------------------------------------&apos;,60,padChar) || &apos;  &apos;||
						RPAD(&apos;------------------------------------------------------------&apos;,60,padChar) || &apos;  &apos;||
						RPAD(&apos;------------------------------------------------------------&apos;,60,padChar);							
		call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, &apos;EXISTING PRIVILEGES:&apos; || message);
		for r as select * from /shared/ASAssets/Utilities/repository/lowerLevelProcedures/getResourcePrivilegesXSLT(
			&apos;&lt;ns1:getResourcePrivileges xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/resource&quot; xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;
				&lt;ns1:entries&gt;
					&lt;ns1:entry&gt;
						&lt;ns1:path&gt;&apos; || resourcelist[i].resource || &apos;&lt;/ns1:path&gt;
						&lt;ns1:type&gt;&apos; || resourcelist[i].resourceType || &apos;&lt;/ns1:type&gt;
					&lt;/ns1:entry&gt;
				&lt;/ns1:entries&gt;
			&lt;/ns1:getResourcePrivileges&gt;&apos;
		) order by nameType, name desc
		do
			set message = 	RPAD(r.&quot;domain&quot;,12,padChar) ||&apos;  &apos;||
							RPAD(r.name,12,padChar) || &apos;  &apos;||
							RPAD(r.nameType,10,padChar) || &apos;  &apos;||
							RPAD(r.privs,60,padChar) || &apos;  &apos;||
							RPAD(r.combinedPrivs,60,padChar) || &apos;  &apos;||
							RPAD(r.inheritedPrivs,60,padChar);							
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, &apos;EXISTING PRIVILEGES:&apos; || message);
		end for;

       /*
        * Step three: Assign privileges and ownership to all resources
        */
        call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, &apos;Assigning PRIVILEGES TO &apos; || resourcelist[i].resource);
        BEGIN
            DECLARE privs VARCHAR(2000) DEFAULT &apos;&apos;;
			set j = 1;
            WHILE j &lt;= CARDINALITY(resourcelist[i].resourceprivs)
            DO
                call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, &apos;    &apos; || resourcelist[i].resourceprivs[j].nameType || &apos; &apos; || resourcelist[i].resourceprivs[j].name || &apos; SET TO &apos; || resourcelist[i].resourceprivs[j].privs);
                SET privs = privs || &apos;
                         &lt;ns1:privilege&gt;
                            &lt;ns1:domain&gt;&apos; || resourcelist[i].resourceprivs[j].domainname || &apos;&lt;/ns1:domain&gt;
                            &lt;ns1:name&gt;&apos; || resourcelist[i].resourceprivs[j].name || &apos;&lt;/ns1:name&gt;
                            &lt;ns1:nameType&gt;&apos; || resourcelist[i].resourceprivs[j].nameType || &apos;&lt;/ns1:nameType&gt;
                            &lt;ns1:privs&gt;&apos; || resourcelist[i].resourceprivs[j].privs || &apos;&lt;/ns1:privs&gt;
                         &lt;/ns1:privilege&gt;&apos;;
                SET j = j + 1;
            END WHILE;
                    
			set request =
               &apos;&lt;ns1:updateResourcePrivileges xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/resource&quot;
                     xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot;
                     xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;
                   &lt;ns1:updateRecursively&gt;&apos; || resourcelist[i].recurse || &apos;&lt;/ns1:updateRecursively&gt;
                   &lt;ns1:privilegeEntries&gt;
                     &lt;ns1:privilegeEntry&gt;
                       &lt;ns1:path&gt;&apos; || resourcelist[i].resource || &apos;&lt;/ns1:path&gt;
                       &lt;ns1:type&gt;&apos; || resourcelist[i].resourceType || &apos;&lt;/ns1:type&gt;
                       &lt;ns1:privileges&gt;&apos; || privs || &apos;
                       &lt;/ns1:privileges&gt;
                     &lt;/ns1:privilegeEntry&gt;
                   &lt;/ns1:privilegeEntries&gt;
                   &lt;ns1:mode&gt;SET_EXACTLY&lt;/ns1:mode&gt;
                &lt;/ns1:updateResourcePrivileges&gt;&apos;;
			CALL /shared/ASAssets/Utilities/log/logDebugMessage(moduleName, debug, request);
            CALL /services/webservices/system/admin/resource/operations/updateResourcePrivileges(request, response, fault);
        EXCEPTION                     
			WHEN System.SOAPFaultException THEN
			RAISE;                                   
        END;
 
        call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, &apos;Assigning ownership TO &apos; || resourcelist[i].resource);
        BEGIN
			set request =
               &apos;&lt;ns1:changeResourceOwner xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/resource&quot;
                     xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot;
                     xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;
                   &lt;ns1:path&gt;&apos; || resourcelist[i].resource || &apos;&lt;/ns1:path&gt;
                   &lt;ns1:type&gt;&apos; || resourcelist[i].resourceType || &apos;&lt;/ns1:type&gt;
                   &lt;ns1:detail&gt;NONE&lt;/ns1:detail&gt;
                   &lt;ns1:newOwnerName&gt;&apos; || owner || &apos;&lt;/ns1:newOwnerName&gt;
                   &lt;ns1:newOwnerDomain&gt;&apos; || ownerDomain || &apos;&lt;/ns1:newOwnerDomain&gt;
                   &lt;ns1:recurse&gt;TRUE&lt;/ns1:recurse&gt;
                &lt;/ns1:changeResourceOwner&gt;&apos;;
			CALL /shared/ASAssets/Utilities/log/logDebugMessage(moduleName, debug, request);
            CALL /services/webservices/system/admin/resource/operations/changeResourceOwner(request, response, fault);
        EXCEPTION                     
			WHEN System.SOAPFaultException THEN
			RAISE;                                   
        END;
        SET i = i + 1;
    END WHILE;

    EXCEPTION
		ELSE
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug, emptyStr(CURRENT_EXCEPTION.TRACE));
			raise ex value CURRENT_EXCEPTION.TRACE;
END</attribute>
