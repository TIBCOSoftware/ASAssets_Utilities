    csserverutil-XXXX.jar, csserver-XXXX.jar, cswebapi-XXXX.jar, and customproc-XXXX.jar (where -XXXX is an 
    optional version number) are provided as a convenience, but are covered under the licensing for the 
    Composite Information Server/Cisco Data Virtualization Server. They cannot be used in any way except 
    through a valid license for that product.

    This software is released AS-IS!. Support for this software is not covered by standard maintenance agreements with Cisco. 
    Any support for this software by Cisco would be covered by paid consulting agreements, and would be billable work.

*/
PROCEDURE addLicense(
	IN debug 				CHAR(1),-- e.g. Y or N
	IN licenseText 			VARCHAR(4096)
)
BEGIN
	DECLARE moduleName 		VARCHAR DEFAULT &apos;addLicense&apos;;
	DECLARE error 			VARCHAR(1024);
	DECLARE request 		VARCHAR(4096);
	DECLARE response 		XML;
	DECLARE fault 			XML;
    DECLARE ex 				EXCEPTION;

	SET request = 
    	&apos;&lt;ns1:addLicenses xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/server&quot; xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;
	    	&lt;ns1:licenseText&gt;&apos; || CAST(XMLTEXT(licenseText) AS VARCHAR(4096)) || &apos;&lt;/ns1:licenseText&gt;
    	&lt;/ns1:addLicenses&gt;&apos;;

	CALL /shared/ASAssets/Utilities/log/logDebugMessage(moduleName, debug, request);
	CALL /services/webservices/system/admin/server/operations/addLicenses(request, response, fault);

	IF (fault IS NOT NULL) THEN
		raise ex value CAST(fault as longvarchar);
	END IF;

	EXCEPTION
	  ELSE
		CALL /shared/ASAssets/Utilities/log/logDebugMessage(moduleName, debug, CURRENT_EXCEPTION.TRACE);
		raise ex value CURRENT_EXCEPTION.TRACE;
END
