  <attribute name="Script" type="STRING">PROCEDURE test_copyResources(
	OUT success		BIT,
	OUT faultResponse 	XML
)
BEGIN
	DECLARE sourcepath 		VARCHAR(2048);
	DECLARE targetBasePath		VARCHAR(2048);

	set sourcepath = &apos;/shared/Utilities/examples/repository/source&apos;;
	set targetBasePath = &apos;/shared/Utilities/examples/repository/target&apos;;

	call print (&apos;COPY: Source=&apos;||sourcepath);
	call print (&apos;COPY: Target=&apos;||targetBasePath);

	call /shared/ASAssets/Utilities/repository/copyResources(sourcepath, targetBasePath, success, faultResponse);

END</attribute>
