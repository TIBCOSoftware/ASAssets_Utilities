		name				VARCHAR(255),
		namespace			LONGVARCHAR,
		nativeType			VARCHAR(255),
		&quot;schema&quot;			LONGVARCHAR,
		referencePath		TypeDefinitions.pathType,
		proprietaryModel	VARBINARY
	);
	DECLARE PUBLIC TYPE pseudoType ROW(
		definition			LONGVARCHAR
	);
*/
