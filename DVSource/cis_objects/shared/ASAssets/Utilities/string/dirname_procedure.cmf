PROCEDURE dirname(
    IN resourcePath /lib/resource/ResourceDefs.ResourcePath,
    OUT result /lib/resource/ResourceDefs.ResourcePath
)
BEGIN
    SET result = RegexFind (resourcePath, &apos;.*(?=[\\/][^\\/]{1,255}[\\/]?$)&apos;, 1);
END</attribute>
